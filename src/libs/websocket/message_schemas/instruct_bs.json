{
    "$schema": "http://json-schema.org/draft-07/schema",
    "$id": "http://example.com/example.json",
    "type": "object",
    "title": "instruct_bs command schema",
    "description": "This command instructs the base station.",
    "default": {},
    "examples": [
        {
            "team_name": "Carologistics",
            "machine": "C-BS",
            "side": "INPUT",
            "base_color": "BASE_RED"
        }
    ],
    "required": [
        "team_name",
        "machine",
        "side",
        "base_color"
    ],
    "properties": {
        "team_name": {
            "type": "string",
            "description": "The name of the team that controls the machine."
        },
        "machine": {
            "type": "string",
            "description": "The name of the machine."
        },
        "side": {
            "type": "string",
            "description": "Machine side to dispense base to."
        },
        "base_color": {
            "type": "string",
            "description": "The base color to be dispensed."
        }
    }
}
