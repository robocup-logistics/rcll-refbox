{
    "$schema": "http://json-schema.org/draft-07/schema",
    "$id": "http://example.com/example.json",
    "type": "object",
    "title": "instruct_rs command schema",
    "description": "This command instructs the ring station.",
    "default": {},
    "examples": [
        {
            "team_name": "Carologistics",
            "machine": "C-RS1",
            "ring_color": "RING_GREEN"
        }
    ],
    "required": [
        "team_name",
        "machine",
        "ring_color"
    ],
    "properties": {
        "team_name": {
            "type": "string",
            "description": "The name of the team that controls the machine."
        },
        "machine": {
            "type": "string",
            "description": "The name of the machine."
        },
        "ring_color": {
            "type": "string",
            "description": "Ring color to dispense."
        }
    }
}
