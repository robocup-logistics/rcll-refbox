{
    "$schema": "http://json-schema.org/draft-07/schema",
    "$id": "http://example.com/example.json",
    "type": "object",
    "title": "add_payment_rs command schema",
    "description": "This command can be used to add bases to machines given the name.",
    "default": {},
    "examples": [
        {
            "command": "add_payment_rs",
            "machine": ""
        }
    ],
    "required": [
        "command",
        "machine"
    ],
    "additionalProperties": true,
    "properties": {
        "command": {
            "$id": "#/properties/command",
            "type": "string",
            "default": "",
            "examples": [
                "add_payment_rs"
            ]
        },
        "mname": {
            "$id": "#/properties/machine",
            "type": "string",
            "default": "",
            "examples": [
                "M-RS1"
            ]
        }
    }
}
